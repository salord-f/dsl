<?xml version="1.0" encoding="ASCII"?>
<rml:Rhythm xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:rml="http://www.polytech.unice.fr/rhythmML" name="MyRythm" BPM="150">
  <tracks xsi:type="rml:DrumTrack" name="MyTrack">
    <sections name="mySection">
      <patternLoops loopNumber="10" pattern="//@tracks.0/@patterns.0"/>
    </sections>
    <patterns name="bar1">
      <beats>
        <ticks>
          <notes xsi:type="rml:DrumNote" drumNote="CH"/>
        </ticks>
        <ticks/>
      </beats>
    </patterns>
    <composition sections="//@tracks.0/@sections.0"/>
    <offsets>
      <note xsi:type="rml:DrumNote" drumNote="CH"/>
    </offsets>
  </tracks>
  <tracks xsi:type="rml:ClassicTrack" name="MyTrack">
    <sections name="mySection2">
      <patternLoops loopNumber="10" pattern="//@tracks.1/@patterns.0"/>
    </sections>
    <patterns name="bar2">
      <beats>
        <ticks/>
        <ticks>
          <notes xsi:type="rml:ClassicNote" pitch="5" note="LA"/>
        </ticks>
      </beats>
    </patterns>
    <composition sections="//@tracks.1/@sections.0"/>
  </tracks>
</rml:Rhythm>
