rhythm MyRythm tempo 100 BPM


drum track MyTrack {

	offsets:
		note: ch +0.0 ticks


	patterns:
		bar1 {
			beat: ch, _, _, bd
		}
		
	sections:
		mySection {
			pattern: bar1 10 times
			
		}
		
		composition:mySection
}

piano track MyTrack {


	patterns:
		bar2 {
			beat: _, _, _, la(5)
			
		}
		
	sections:
		mySection2 {
			pattern: bar2 10 times
			
			
			
		}
		
		composition:mySection2
}