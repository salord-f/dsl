rhythm MyRythm tempo 150 BPM
resolution: 2
offset: 0.1 tick
track MyTrack {
	patterns:
		bar1 {
			beat: bd,_,sd,_
			beat: ch,_,
		}
		ttt{
			beat: ch
		}
	sections:
		mySection {
			pattern: bar1 5 times
			bar1 (iteration:2,beat:0): bd,_,bd,_
		}
		mySection2 {
			pattern: ttt 5 times
			bar1 (iteration:0,beat:0): bd,_
		}
		composition:mySection, mySection2
}